{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "editor.background": "#0F1219",
        "editor.foreground": "#abb2bf",
        "editorLineNumber.foreground": "#495162",
        "editorLineNumber.activeForeground": "#abb2bf",
        "editorCursor.foreground": "#5ab0f6",
        "editor.selectionBackground": "#2c313a",
        "editor.selectionHighlightBackground": "#2c313a",
        "editorIndentGuide.background": "#1e242e",
        "editorIndentGuide.activeBackground": "#3e4452",
        "statusBar.background": "#0F1219",
        "statusBar.foreground": "#abb2bf",
        "activityBar.background": "#0F1219",
        "activityBar.foreground": "#abb2bf",
        "activityBar.border": "#1e242e",
        "sideBar.background": "#0F1219",
        "sideBar.foreground": "#abb2bf",
        "sideBar.border": "#1e242e",
        "sideBarSectionHeader.background": "#0F1219",
        "sideBarSectionHeader.foreground": "#abb2bf",
        "sideBarSectionHeader.border": "#1e242e",
        "editorGroupHeader.tabsBackground": "#0F1219",
        "editorGroupHeader.noTabsBackground": "#0F1219",
        "tab.activeBackground": "#1e242e",
        "tab.inactiveBackground": "#0F1219",
        "tab.activeForeground": "#abb2bf",
        "tab.inactiveForeground": "#495162",
        "tab.border": "#1e242e",
        "titleBar.activeBackground": "#0F1219",
        "titleBar.activeForeground": "#abb2bf",
        "titleBar.inactiveBackground": "#0F1219",
        "titleBar.inactiveForeground": "#495162",
        "titleBar.border": "#1e242e",
        "list.activeSelectionBackground": "#2c313a",
        "list.inactiveSelectionBackground": "#1e242e",
        "list.hoverBackground": "#1e242e",
        "list.activeSelectionForeground": "#abb2bf",
        "list.inactiveSelectionForeground": "#abb2bf",
        "scrollbarSlider.background": "#1e242e",
        "scrollbarSlider.hoverBackground": "#1e242e",
        "scrollbarSlider.activeBackground": "#2c313a",
        "panel.background": "#0F1219",
        "panel.border": "#1e242e",
        "panelTitle.activeForeground": "#abb2bf",
        "panelTitle.inactiveForeground": "#495162",
        "dropdown.background": "#0F1219",
        "dropdown.border": "#1e242e",
        "input.background": "#0F1219",
        "input.border": "#1e242e",
        "button.background": "#0F1219",
        "button.foreground": "#abb2bf",
        "button.border": "#1e242e",
        "badge.background": "#0F1219",
        "badge.foreground": "#abb2bf",
        "breadcrumb.background": "#0F1219",
        "breadcrumb.foreground": "#abb2bf",
        "menu.background": "#0F1219",
        "menu.foreground": "#abb2bf",
        "menu.border": "#1e242e",
        "menubar.selectionBackground": "#2c313a",
        "quickInput.background": "#0F1219",
        "quickInput.foreground": "#abb2bf",
        "editorWidget.background": "#0F1219",
        "editorWidget.border": "#1e242e",
        "notifications.background": "#0F1219",
        "notifications.border": "#1e242e",
        "statusBar.border": "#1e242e",
        "window.activeBorder": "#1e242e",
        "window.inactiveBorder": "#1e242e"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment"],
            "settings": {
                "foreground": "#546178",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ca72e4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#5ab0f6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#d99a5e"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "name": "String Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "name": "Special Strings",
            "scope": [
                "string.special",
                "string.quoted.other.special"
            ],
            "settings": {
                "foreground": "#56b6c2"
            }
        },
        {
            "name": "Types and Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#ebc275"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ebc275"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#abb2bf"
            }
        }
    ]
}
