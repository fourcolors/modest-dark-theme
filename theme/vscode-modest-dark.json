{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "editor.background": "#0F1219",
        "editor.foreground": "#abb2bf",
        "editorLineNumber.foreground": "#495162",
        "editorLineNumber.activeForeground": "#abb2bf",
        "editorCursor.foreground": "#5ab0f6",
        "editorCursor.background": "#0F121980",
        "editor.selectionBackground": "#2c313a80",
        "editor.selectionHighlightBackground": "#2c313a80",
        "editorIndentGuide.background1": "#1e242e",
        "editorIndentGuide.activeBackground1": "#3e4452",
        "statusBar.background": "#0F1219",
        "statusBar.foreground": "#abb2bf",
        "statusBar.border": "#1e242e",
        "activityBar.background": "#0F1219",
        "activityBar.foreground": "#abb2bf",
        "activityBar.border": "#1e242e",
        "activityBarBadge.background": "#5ab0f6",
        "activityBarBadge.foreground": "#0F1219",
        "sideBar.background": "#0F1219",
        "sideBar.foreground": "#abb2bf",
        "sideBar.border": "#1e242e",
        "sideBarSectionHeader.background": "#0F1219",
        "sideBarSectionHeader.foreground": "#abb2bf",
        "sideBarSectionHeader.border": "#1e242e",
        "sideBarTitle.foreground": "#abb2bf",
        "list.activeSelectionBackground": "#2c313a",
        "list.activeSelectionForeground": "#abb2bf",
        "list.inactiveSelectionBackground": "#1e242e",
        "list.inactiveSelectionForeground": "#abb2bf",
        "list.hoverBackground": "#1e242e",
        "list.hoverForeground": "#abb2bf",
        "list.focusBackground": "#2c313a",
        "list.focusForeground": "#abb2bf",
        "list.highlightForeground": "#5ab0f6",
        "list.errorForeground": "#e06c75",
        "list.warningForeground": "#ebc275",
        "tree.indentGuidesStroke": "#1e242e",
        "editorGroupHeader.tabsBackground": "#0F1219",
        "editorGroupHeader.noTabsBackground": "#0F1219",
        "editorGroupHeader.tabsBorder": "#1e242e",
        "editorGroup.border": "#1e242e",
        "editorGroup.dropBackground": "#0F121980",
        "tab.activeBackground": "#1e242e",
        "tab.inactiveBackground": "#0F1219",
        "tab.activeForeground": "#abb2bf",
        "tab.inactiveForeground": "#495162",
        "tab.border": "#1e242e",
        "tab.activeBorder": "#5ab0f6",
        "tab.unfocusedActiveBorder": "#1e242e",
        "scrollbarSlider.background": "#1e242e80",
        "scrollbarSlider.hoverBackground": "#2c313a80",
        "scrollbarSlider.activeBackground": "#3e445280",
        "minimapSlider.background": "#1e242e80",
        "minimapSlider.hoverBackground": "#2c313a80",
        "minimapSlider.activeBackground": "#3e445280",
        "editorOverviewRuler.border": "#1e242e",
        "panel.background": "#0F1219",
        "panel.border": "#1e242e",
        "panelTitle.activeForeground": "#abb2bf",
        "panelTitle.inactiveForeground": "#495162",
        "panelTitle.activeBorder": "#5ab0f6",
        "titleBar.activeBackground": "#0F1219",
        "titleBar.activeForeground": "#abb2bf",
        "titleBar.inactiveBackground": "#0F1219",
        "titleBar.inactiveForeground": "#495162",
        "titleBar.border": "#1e242e",
        "menu.background": "#0F1219",
        "menu.foreground": "#abb2bf",
        "menu.border": "#1e242e",
        "menu.selectionBackground": "#2c313a",
        "menu.selectionForeground": "#abb2bf",
        "menu.selectionBorder": "#1e242e",
        "menu.separatorBackground": "#1e242e",
        "quickInput.background": "#0F1219",
        "quickInput.foreground": "#abb2bf",
        "quickInputList.focusBackground": "#2c313a",
        "dropdown.background": "#0F1219",
        "dropdown.foreground": "#abb2bf",
        "dropdown.border": "#1e242e",
        "input.background": "#0F1219",
        "input.foreground": "#abb2bf",
        "input.border": "#1e242e",
        "input.placeholderForeground": "#495162",
        "inputOption.activeBorder": "#5ab0f6",
        "button.background": "#5ab0f6",
        "button.foreground": "#0F1219",
        "button.hoverBackground": "#5ab0f6e6",
        "badge.background": "#5ab0f6",
        "badge.foreground": "#0F1219",
        "progressBar.background": "#5ab0f6"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#546178",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "variable.other.constant",
                "variable.language",
                "meta.definition.variable"
            ],
            "settings": {
                "foreground": "#d99a5e"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ca72e4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "punctuation.support.type.property-name",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "string.quoted.double.json punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "scope": [
                "support.type.property-name",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language.json"
            ],
            "settings": {
                "foreground": "#d99a5e"
            }
        },
        {
            "scope": [
                "punctuation.separator",
                "punctuation.definition.dictionary",
                "meta.structure.dictionary.json punctuation.definition.dictionary"
            ],
            "settings": {
                "foreground": "#abb2bf"
            }
        },
        {
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#5ab0f6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#d99a5e"
            }
        },
        {
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#97ca72"
            }
        },
        {
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#ebc275"
            }
        }
    ]
}
